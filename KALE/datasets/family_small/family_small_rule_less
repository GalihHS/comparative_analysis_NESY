husband(y,x)==>wife(x,y)
wife(y,x)==>husband(x,y)
husband(x,y)&&mother(y,z)==>father(x,z)
husband(x,y)&&daughter(z,y)==>father(x,z)
husband(x,y)&&aunt(y,z)==>uncle(x,z)
husband(x,y)&&niece(z,y)==>uncle(x,z)
sister(x,y)&&mother(y,z)==>aunt(x,z)
son(x,y)&&husband(y,z)==>son(x,z)
daughter(x,y)&&husband(y,z)==>daughter(x,z)
brother(x,y)&&daughter(y,z)==>son(x,z)
sister(x,y)&&daughter(y,z)==>daughter(x,z)
sister(x,y)&&sister(y,z)==>sister(x,z)
daughter(x,y)&&mother(y,z)==>sister(x,z)
