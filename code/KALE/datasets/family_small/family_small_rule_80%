daughter(x,y)&&wife(y,z)==>daughter(x,z)
daughter(x,y)&&husband(y,z)==>daughter(x,z)
brother(x,y)&&son(y,z)==>son(x,z)
brother(x,y)&&daughter(y,z)==>son(x,z)
sister(x,y)&&son(y,z)==>daughter(x,z)
sister(x,y)&&daughter(y,z)==>daughter(x,z)
