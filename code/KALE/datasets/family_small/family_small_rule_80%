sister(x,y)&&sister(y,z)==>sister(x,z)
sister(x,y)&&sister(z,y)==>sister(x,z)
sister(x,y)&&brother(y,z)==>sister(x,z)
sister(x,y)&&brother(z,y)==>sister(x,z)
